{
	"arrowParens": "always",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"endOfLine": "auto",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"jsxSingleQuote": false,
	"printWidth": 140,
	"proseWrap": "always",
	"quoteProps": "consistent",
	"requirePragma": false,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 3,
	"useTabs": true,
	"trailingComma": "all",
	"importOrder": ["^(^react$|@react|react|^recoil|redux|vite$)", "<THIRD_PARTY_MODULES>", "^@@(.*)$", "^[./]"],
	"importOrderGroupNamespaceSpecifiers": true,
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
